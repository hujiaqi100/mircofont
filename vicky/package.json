    "main": "index.js",
    "scripts": {
        "prodBuild": "rm -rf out & mkdir out & npm-run-all --parallel prod:**",
        "prod:build:client": "webpack --mode production --config ./build/webpack.common.js",
        "prod:build:server": "cp server.js out & cp -r middleware out",
        "build": "webpack --mode production --config ./build/webpack.common.js",
        "devBuild:s": "webpack  --config ./build/webpack.common.js",
        "dev-s": "webpack-dev-server --mode development --config ./build/webpack.common.js",
        "dev": "NODE_ENV=development node index.js",
        "serverHttp": "http-server dist",
        "build:dll": "webpack  --config ./build/webpack.dll.js",
        "debug": "node --inspect --inspect-brk  node_modules/webpack/bin/webpack.js --config ./build/webpack.common.js",
        "server": "npm-run-all --parallel dev:**",
        "dev:build:client": "webpack --mode production --config ./build/webpack.common.js --watch",
        "dev:start:server": "node index.js",
        "roll": "rollup -c",
        "start": "nodemon --config nodemon.json server.js",
        "starts": "webpack serve --config ./build/webpack.common.js"
    },
